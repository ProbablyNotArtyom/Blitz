s%@MAINTAINER_MODE_TRUE@%$MAINTAINER_MODE_TRUE%;t t
s%@MAINTAINER_MODE_FALSE@%$MAINTAINER_MODE_FALSE%;t t
s%@MAINT@%$MAINT%;t t
s%@WANT_MPBSD_TRUE@%$WANT_MPBSD_TRUE%;t t
s%@WANT_MPBSD_FALSE@%$WANT_MPBSD_FALSE%;t t
s%@CC@%$CC%;t t
s%@CCAS@%$CCAS%;t t
s%@CPP@%$CPP%;t t
s%@LN_S@%$LN_S%;t t
s%@M4@%$M4%;t t
s%@AR@%$AR%;t t
s%@NM@%$NM%;t t
s%@EXEEXT@%$EXEEXT%;t t
s%@OBJEXT@%$OBJEXT%;t t
