	$(ROMFSINST) -e CONFIG_USER_RAMIMAGE_RAMFS256 ramfs256.img /etc/ramfs.img
	$(ROMFSINST) -e CONFIG_USER_RAMIMAGE_RAMFS512 ramfs512.img /etc/ramfs.img
	$(ROMFSINST) -e CONFIG_USER_RAMIMAGE_RAMFS1024 ramfs1024.img /etc/ramfs.img
	$(ROMFSINST) -e CONFIG_USER_RAMIMAGE_RAMFS2048 ramfs2048.img /etc/ramfs.img
	$(ROMFSINST) -e CONFIG_USER_RAMIMAGE_RAMFS8192 ramfs8192.img /etc/ramfs.img

