_ACEOF

	    cat >>confdefs.h <<\_ACEOF
#define HAVE_DEC_3_2_THREADS 1
_ACEOF

            with_osf32_threads="yes"
            MYSQLD_DEFAULT_SWITCHES="--skip-thread-priority"
	    echo "$as_me:$LINENO: result: \"yes\"" >&5
echo "${ECHO_T}\"yes\"" >&6
	  else
	    echo "$as_me:$LINENO: result: \"no\"" >&5
echo "${ECHO_T}\"no\"" >&6
